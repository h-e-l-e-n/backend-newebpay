{
    "version": 2,
    "builds": [
    { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
    { "src": "/payment", "dest": "/app.js" },
    { "src": "/createOrder", "dest": "/app.js" },
    { "src": "/check/(.*)", "dest": "/app.js" },
    { "src": "/newebpay_return", "dest": "/app.js" },
    { "src": "/(.*)", "dest": "/app.js" }
    ]
  }
  